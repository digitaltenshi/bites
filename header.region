<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Google Double Click Ad management -->
    <script>
      (function() {
        var useSSL = 'https:' == document.location.protocol;
        var src = (useSSL ? 'https:' : 'http:') +
            '//www.googletagservices.com/tag/js/gpt.js';
        document.write('<scr' + 'ipt src="' + src + '"></scr' + 'ipt>');
      })();
    </script>

    <script>
      googletag.cmd.push(function() {
        //Homepage Ads
        googletag.defineSlot('/11160298/Sidebar_300x250_Home_1', [300, 250], 'div-gpt-ad-1474687809144-0').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Sidebar_300x250_Home_2', [300, 250], 'div-gpt-ad-1474687809144-1').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Sidebar_300x250_Home_3', [300, 250], 'div-gpt-ad-1474687809144-2').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Sidebar_300x250_Home_4', [300, 250], 'div-gpt-ad-1474687809144-3').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_728x90_Home', [[468, 60], [728, 90]], 'div-gpt-ad-1474687809144-4').addService(googletag.pubads());

        //Directory Page Ads
        /*googletag.defineSlot('/11160298/Sidebar_300x250_Directory_1', [300, 250], 'div-gpt-ad-1475086344521-4').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Sidebar_300x250_Directory_2', [300, 250], 'div-gpt-ad-1475086344521-5').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_728x90_Directory', [728, 90], 'div-gpt-ad-1475086344521-0').addService(googletag.pubads());*/
        
        //Features Page Ads
        googletag.defineSlot('/11160298/Sidebar_300x250_Features_1', [300, 250], 'div-gpt-ad-1475086344521-6').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_728x90_Features', [[468, 60], [728, 90]], 'div-gpt-ad-1475086344521-1').addService(googletag.pubads());

        //Restaurant Page Ads
        googletag.defineSlot('/11160298/Sidebar_300x250_Restaurants_1', [300, 250], 'div-gpt-ad-1475086344521-8').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_728x90_Restaurants', [[468, 60], [728, 90]], 'div-gpt-ad-1475086344521-3').addService(googletag.pubads());

        //Calendar and Event pages
        googletag.defineSlot('/11160298/Sidebar_Calendar', [300, 250], 'div-gpt-ad-1487020702612-2').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Sidebar_Event', [300, 250], 'div-gpt-ad-1487020702612-3').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_Calendar', [[468, 60], [728, 90]], 'div-gpt-ad-1487020702612-0').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_Event', [[468, 60], [728, 90]], 'div-gpt-ad-1487020702612-1').addService(googletag.pubads());
        
        
        // Generic Page Ads
        googletag.defineSlot('/11160298/Sidebar_300x250_General_1', [300, 250], 'div-gpt-ad-1475086344521-7').addService(googletag.pubads());
        googletag.defineSlot('/11160298/Banner_728x90_General', [[468, 60], [728, 90]], 'div-gpt-ad-1475086344521-2').addService(googletag.pubads());
              
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
      });
    </script>

    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=1251863578234441";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>

    <div class="container">

      <header id="header" class="header">
        <div id = "header-inside">
        <!--Site title or logo.-->
        {.section website}

          <h1 class="site-title" data-content-field="site-title">
            <a href="/">
              {.section logoImageUrl}
               <img class="logo" src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
                {siteTitle}
              {.end}
            </a>
          </h1>
          
          <!--Site tagline.-->
          {.section siteTagLine}
            <p class="site-tagline">{@}</p>
          {.end}

        {.end}

        <!--Main navigation.-->
        <squarespace:navigation navigationId="mainNav" template="navigation" />
      </div>
      </header>
