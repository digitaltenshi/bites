<div id="page-body-wrapper" class="one-sidebar">
  <div id="page-body" class = "sqs-col sqs-col-12">
    <div id="content-wrapper">
      <div id="content-restaurants" class = "sqs-col sqs-col-12">
        <div id="page-body-header" class="extra-wrapper">
          <squarespace:block-field id="page-header-{collection.id}" description="Page Header" columns="12" />
        </div>
        <div class="clearer sqs-col sqs-col-12">&nbsp;</div>
        <div class = "sqs-col sqs-col-8" id = "main-column">
          {squarespace.main-content}
        </div>
        <div id="sidebar-one-wrapper" class="vertical-navigation-bar-wrapper sqs-col sqs-col-3">
          <div id="sidebar-one" class="vertical-navigation-bar">
            <div class="sponsors">
              <h3>Our Sponsors</h3>
              <!-- /11160298/Sidebar_300x250_Features_1 -->
              <div id='div-gpt-ad-1475086344521-6' class='google-ad--hidden'></div>
              <!-- /11160298/Sidebar_300x250_Restaurants_1 -->
              <div id='div-gpt-ad-1475086344521-8' class='google-ad--hidden'></div>
              <!-- /11160298/Sidebar_300x250_General_1 -->
              <div id='div-gpt-ad-1475086344521-7' class='google-ad--hidden'></div>
              <script>
                var hasSpecialtyAd = false;
                if ($('article').hasClass('c_features')) {
                  googletag.cmd.push(function() { googletag.display('div-gpt-ad-1475086344521-6'); });
                  $('#div-gpt-ad-1475086344521-6').removeClass('google-ad--hidden');
                  $('#div-gpt-ad-1475086344521-6').addClass('google-ad--shown');
                  hasSpecialtyAd = true;
                } else if ($('article').hasClass('c_restaurant')) {
                  googletag.cmd.push(function() { googletag.display('div-gpt-ad-1475086344521-8'); });
                  $('#div-gpt-ad-1475086344521-8').removeClass('google-ad--hidden');
                  $('#div-gpt-ad-1475086344521-8').addClass('google-ad--shown');
                  hasSpecialtyAd = true;
                } 
                if (!hasSpecialtyAd) {
                  googletag.cmd.push(function() { googletag.display('div-gpt-ad-1475086344521-7'); });
                  $('#div-gpt-ad-1475086344521-7').removeClass('google-ad--hidden');
                  $('#div-gpt-ad-1475086344521-7').addClass('google-ad--shown');
                }        
              </script>
              </div>
            </div>
            
            {.section website}
            {.end}
            <div id="regform-wrapper" class = "sqs-col sqs-col-12">
              <div id="reg-form" class = "sqs-col sqs-col-12">
                <squarespace:block-field id="regform"  />
              </div>
            </div>
            
            <squarespace:block-field id="sidebar-one-blocks" description="The block field for sidebar 1" />
          </div>
          </div><!-- sidebar-one-wrapper -->
          
          </div> <!-- content-wrapper -->
          <div class="clearer" id="body-clearer"></div>
          {@|apply editions.block}
          {@|apply bottom-banner-general.block}
        </div>
        {@|apply searchoverlay.block}
        </div> <!-- page-body-wrapper -->
        <div class="clearer sqs-col sqs-col-12">&nbsp;</div>
      </div>